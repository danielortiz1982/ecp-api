e":"rac_body-text_bulleted","elements":[{"type":"text","text":"Be careful when I cross the street. "}]}]},{"type":"element","name":"section","elements":[{"type":"element","name":"rac_column_heds_with_rule","elements":[{"type":"text","text":"Independent Writing "}]},{"type":"element","name":"rac_body-text","elements":[{"type":"text","text":"Invite children to draw or write about how to stay safe and share their work. "},{"type":"element","name":"rac_child_development_icon"}]}]}]},{"type":"element","name":"rac_support_challenge","elements":[{"type":"element","name":"rac_support_challenge-box","elements":[{"type":"element","name":"rac_column_hed_support-challenge","elements":[{"type":"text","text":"Support "}]},{"type":"element","name":"rac_support_numbers","elements":[{"type":"text","text":"1"}]},{"type":"element","name":"rac_teacher-talk_blue","elements":[{"type":"text","text":"Is it safe to stand on a chair?"}]}]},{"type":"element","name":"rac_support_challenge-box","elements":[{"type":"element","name":"rac_support_numbers","elements":[{"type":"text","text":"2"}]},{"type":"element","name":"rac_teacher-talk_blue","elements":[{"type":"text","text":"It is not s"},{"type":"element","name":"fill-in-the-blank","elements":[{"type":"text","text":"____"}]},{"type":"element","name":"rac_teacher-talk_emphasis_non-ital","elements":[{"type":"text","text":"(safe) "}]},{"type":"text","text":"to stand on a chair."}]}]},{"type":"element","name":"rac_support_challenge-box","elements":[{"type":"element","name":"rac_support_numbers","elements":[{"type":"text","text":"3"}]},{"type":"element","name":"rac_teacher-talk_blue","elements":[{"type":"text","text":"It is not safe to stand on a chair."}]}]},{"type":"element","name":"rac_support_challenge-box","elements":[{"type":"element","name":"rac_column_hed_support-challenge","elements":[{"type":"text","text":"Challenge "}]},{"type":"element","name":"rac_teacher-talk_blue","elements":[{"type":"text","text":"What are things you can do to be ca